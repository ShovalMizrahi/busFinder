<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layoutDirection="ltr"
    android:orientation="vertical"
    tools:context=".ui.notifications.NotificationsFragment">

    <EditText
        android:id="@+id/start_station"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:ems="10"
        android:hint="start station"
        android:inputType="textPersonName"
        android:minHeight="48dp"
        android:onClick="startAutoCompleteActivity" />

    <Button
        android:id="@+id/button8"
        android:layout_width="136dp"
        android:layout_height="70dp"
        android:layout_marginEnd="21dp"
        android:onClick="startAutoCompleteActivity"
        android:text="search destination" />

    <EditText
        android:id="@+id/end_station"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="end station"
        android:inputType="textPersonName"
        android:minHeight="48dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="150dp"
        android:layout_height="53dp"

        android:onClick="startAutoCompleteActivity"
        android:text="search start" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="findRoute"
        android:text="search route" />

    <TextView
        android:id="@+id/routeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxLines="10"
        android:text="TextView" />

</LinearLayout>